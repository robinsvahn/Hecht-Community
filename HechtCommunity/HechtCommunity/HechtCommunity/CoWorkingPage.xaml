<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HechtCommunity.CoWorkingPage"
             Title="CoWorking">
  <!--Pages can be added as references or inline-->
    <ContentPage Title="Stube" BackgroundColor="#FFFAD8">
        <Grid Padding="20">
            <Grid.RowDefinitions>
                <RowDefinition Height="3*"/>
                <RowDefinition Height="2*"/>
                <RowDefinition Height="5*"/>
                <RowDefinition Height="1*"/>
                <RowDefinition Height="5*"/>
                <RowDefinition Height="1*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
            </Grid.ColumnDefinitions>

            <Button
                x:Name="TableOneBtn"
                Grid.Row="0"
                Grid.Column="1"
                Grid.ColumnSpan="2"
                BackgroundColor="#0e5139"
                Clicked="TableOneBtn_OnClicked"/>

            <Button
                x:Name="TableTwoBtn"
                Grid.Row="2"
                Grid.Column="0"
                BackgroundColor="#0e5139"
                Clicked="TableTwoBtn_OnClicked"/>

            <Button
                x:Name="TableThreeBtn"
                Grid.Row="2"
                Grid.Column="2"
                BackgroundColor="#0e5139"
                Clicked="TableThreeBtn_OnClicked"/>

            <Button
                x:Name="TableFourBtn"
                Grid.Row="4"
                Grid.Column="0"
                BackgroundColor="#0e5139"
                Clicked="TableFourBtn_OnClicked"/>

            <Button
                x:Name="TableFiveBtn"
                Grid.Row="4"
                Grid.Column="2"
                BackgroundColor="#0e5139"
                Clicked="TableFiveBtn_OnClicked"/>

        </Grid>

        


    </ContentPage>
    <ContentPage Title="OG" />
    <ContentPage Title="EG" />
</TabbedPage>